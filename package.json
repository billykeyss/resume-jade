{
  "name": "resume-jade",
  "version": "0.0.0",
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.x"
  },
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "eslint **/*.js",
    "watch": "sass --watch public/sass:public/stylesheets --style compressed",
    "watchJS": "watch 'npm run uglyifyJs -s' public/javascripts",
    "uglyifyJs": "uglifyjs public/javascripts/lib/webfont.min.js public/javascripts/lib/jquery.min.js public/javascripts/lib/jquery.scrollTo.min.js public/javascripts/lib/Chart.bundle.min.js public/javascripts/lib/particles.min.js public/javascripts/main.js --compress --mangle --output public/main.min.js",
    "build": ""
  },
  "dependencies": {
    "aws-sdk": "^2.141.0",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "jade": "~1.11.0",
    "memory-cache": "~0.1.6",
    "morgan": "~1.7.0",
    "moviedb": "^0.2.10",
    "npm": "^5.8.0",
    "request": "^2.83.0",
    "serve-favicon": "~2.3.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "dotenv": "^5.0.0",
    "watch": "^1.0.2"
  }
}
